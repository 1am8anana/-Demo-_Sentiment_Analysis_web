{
    "version": 2,
    "functions": {
        "api/index.py": {
            "maxLambdaSize": "50mb"
        }
    },
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "Classifier/Naive Bayes_pipeline.pickle",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}